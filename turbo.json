{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".dist/**"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "start": {
      "dependsOn": ["build"],
      "outputs": []
    }
  },
  "globalDependencies": [
    ".env",
    ".eslintrc",
    "tsconfig.json"
  ]
}
